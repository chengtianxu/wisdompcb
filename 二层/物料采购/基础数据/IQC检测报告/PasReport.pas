procedure TfrmReporter.Get2DCodeBmp(AText: string; APic: TPicture);
var
  LCode2d: TQRCodeGraphic;
  LbtMap: TGraphic;
begin
  LCode2d := TQRCodeGraphic.Create();
  try
    LCode2d.Margin := 3;
    LCode2d.BlockSize := 2;
    LCode2d.Text := AText;
